{
  "extends": ["next/core-web-vitals", "next", "prettier"],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "no-multiple-empty-lines": ["error", { "max": 1 }],
    "no-trailing-spaces": "error",
    "eol-last": ["error", "always"],
    "indent": ["error", 2]
  }
}
